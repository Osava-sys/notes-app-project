{
  "name": "notes-app-frontend",
  "version": "1.0.0",
  "description": "Mobile app for student grades management",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "ionic:serve": "ng serve",
    "ionic:build": "ionic build",
    "cap:add": "ionic cap add android",
    "cap:sync": "ionic cap sync",
    "cap:open": "ionic cap open android",
    "cap:run": "ionic cap run android",
    "android:build": "npm run build:prod && npm run cap:sync && cd android && ./gradlew assembleDebug",
    "android:release": "npm run build:prod && npm run cap:sync && cd android && ./gradlew assembleRelease"
  },
  "private": true,
  "dependencies": {
